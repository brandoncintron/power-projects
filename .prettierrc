{
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "useTabs": false,
  "endOfLine": "auto",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^react$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/(components|lib|hooks|utils|schema|types|actions)/(.*)$",
    "",
    "^@@/(.*)$",
    "",
    "^[.]"
  ]
}
